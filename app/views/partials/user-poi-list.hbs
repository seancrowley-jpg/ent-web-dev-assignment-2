<div class="uk-margin uk-width-2xlarge uk-margin-auto uk-card uk-card-default uk-card-body uk-box-shadow-large">
    <table class="uk-table">
        <caption>
            A list of your Points of Interest
        </caption>
        <thead>
        <th>Name</th>
        <th>Description</th>
        <th>View Poi</th>
        <th>Edit Poi</th>
        <th>Delete Poi</th>
        </thead>
        <tbody class="uk-text-left">
        {{#each poi}}
            <tr>
                <td>{{name}}</td>
                <td>{{description}}</td>
                <td><a href="user-poi/{{_id}}"><button class="uk-button uk-button-primary uk-button-small">View</button></a> </td>
                <td><a href="update-poi/{{_id}}"><button class="uk-button uk-button-primary uk-button-small">Edit</button></a> </td>
                <td><a href="deletePoi/{{_id}}"><button class="uk-button-danger uk-button-primary uk-button-small">Delete</button></a> </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>